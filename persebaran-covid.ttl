@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org> .
@prefix dbo: <https://dbpedia.org/ontology> .
@prefix dbr: <https://dbpedia.org/resource/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix covid: <https://raw.githubusercontent.com/rhdayat/linkeddatacovid19/main/persebaran-covid.ttl#> .

#################################################
#        Class Persebaran Covid Provinsi        #
#################################################
covid:Covid_Case_In_Indonesia_Province a rdfs:Class .
covid:Covid_Case_In_Indonesia_Province rdfs:subClassof schema:SpecialAnnouncement .

covid:totalPatientHealed a rdf:Property .
covid:totalPatientHealed rdfs:domain covid:Covid_Case_In_Indonesia_Province .
covid:totalPatientHealed rdfs:range xsd:integer .

covid:totalConfirmedCase a rdf:Property .
covid:totalConfirmedCase rdfs:domain covid:Covid_Case_In_Indonesia_Province .
covid:totalConfirmedCase rdfs:range xsd:integer .

covid:totalActiveCase a rdf:Property .
covid:totalActiveCase rdfs:domain covid:Covid_Case_In_Indonesia_Province .
covid:totalActiveCase rdfs:range xsd:integer .

covid:totalPatientDeath a rdf:Property .
covid:totalPatientDeath rdfs:domain covid:Covid_Case_In_Indonesia_Province .
covid:totalPatientDeath rdfs:range xsd:integer .

covid:disease a rdf:Property .
covid:disease rdfs:domain covid:Covid_Case_In_Indonesia_Province, covid:Covid19_Symptom_Statistics .
covid:disease rdfs:range dbo:disease .
#################################################


